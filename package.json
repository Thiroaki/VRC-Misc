{
  "name": "VRC-Misc",
  "version": "1.2.2",
  "description": "VRCMisc",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "electron-builder build --win --publish never",
    "deploy": "electron-builder build --win --publish always"
  },
  "author": "okou",
  "license": "",
  "devDependencies": {
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "electron-reload": "^1.5.0"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "bootstrap": "^4.4.1",
    "chokidar": "^3.4.0",
    "dateformat": "^3.0.3",
    "electron-log": "^4.1.2",
    "electron-store": "^5.1.1",
    "electron-updater": "^4.3.1",
    "fs-extra": "^8.1.0",
    "jquery": "^3.5.1",
    "log4js": "^6.2.1",
    "node-cron": "^2.0.3",
    "popper.js": "^1.16.1",
    "request": "^2.88.2"
  },
  "build": {
    "files": [
      "src/",
      "main.js",
      "icon.ico",
      "node_modules",
      "package.json"
    ],
    "publish": [
      {
        "provider": "github",
        "private": true,
        "owner": "Thiroaki",
        "repo": "VRC-Misc"
      }
    ],
    "win": {
      "publish": [
        "github"
      ],
      "icon": "icon.ico",
      "target": {
        "target": "nsis",
        "arch": [
          "x64"
        ]
      }
    }
  }
}
